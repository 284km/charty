   "source": [
    "bar = charty.bar do\n",
    "  xlabel 'foo'\n",
    "  ylabel 'bara'\n",
    "  series [0, 1, 4,  2], [10, 20, 60, 70]\n",
    "  series [0, 1, 2], [30, 40, 50]\n",
    "  title 'this is the title'\n",
    "end\n",
    "IRuby.display(IRuby.html(bar.render))"
   ]
